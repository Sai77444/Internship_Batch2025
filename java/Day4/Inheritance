class sai{
	    public sai(String name, String model) {
	        this.name = name;
	        this.model = model;
	    }

}
  class Bike extends sai{
	  String company;

	    public Bike(String name, String model, String company) {
	        super(name, model);
	        this.company = company;
	    }

	    void display1() {
	        System.out.println("Name: " + name);
	        System.out.println("Model: " + model);
	        System.out.println("Company: " + company);
	    }
	}
  class Car extends sai{
	  String company;

	    public Car(String name, String model, String company) {
	        super(name, model);
	        this.company = company;
	    }

	    void display2() {
	        System.out.println("Name: " + name);
	        System.out.println("Model: " + model);
	        System.out.println("Company: " + company);
	    }
	}
  public class Vechile11{
	  public static void main(String[] args) {
	        Bike b = new Bike("Sai", "Sport", "Yamaha");
	        Car c = new Car("Sai", "Sedan", "Hyundai");

	        b.display1();
	        c.display2();
	    }
	}

  
